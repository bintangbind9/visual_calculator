{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "visual_calculator",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "visual_calculator (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "visual_calculator (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "visual_calculator (debug mode on Chrome)",
      "program": "lib/main.dart",
      "deviceId": "chrome",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": [
        "--web-browser-flag=--disable-web-security",
        "--web-port",
        "63330",
      ]
    }
  ]
}